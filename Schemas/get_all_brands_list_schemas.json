{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Brands List Schema",
    "type": "object",
    "properties": {
        "responseCode": {
            "type": "integer",
            "description": "HTTP-style response code indicating success or failure",
            "enum": [200]
        },
        "brands": {
            "type": "array",
            "description": "List of available brands with their IDs",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "description": "Unique brand identifier"
                    },
                    "brand": {
                        "type": "string",
                        "description": "Brand name"
                    }
                },
                "required": ["id", "brand"],
                "additionalProperties": false
            },
            "minItems": 1
        }
    },
    "required": ["responseCode", "brands"],
    "additionalProperties": false
}
