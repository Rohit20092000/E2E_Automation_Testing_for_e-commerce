{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Search Product Missing Parameter Schema",
  "type": "object",
  "properties": {
    "responseCode": {
      "type": "integer",
      "const": 400,
      "description": "Indicates a bad request when a required parameter is missing"
    },
    "message": {
      "type": "string",
      "pattern": ".*missing.*search_product.*",
      "description": "Error message for missing search_product parameter"
    }
  },
  "required": ["responseCode", "message"],
  "additionalProperties": false
}
